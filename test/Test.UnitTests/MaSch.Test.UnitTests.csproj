<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- General Project info -->
    <TargetFrameworks>$(TfmNetCoreApp);$(TfmNetFramework)</TargetFrameworks>
    <EnableMaSchTest>false</EnableMaSchTest>
    <EnableMaSchGenerators>true</EnableMaSchGenerators>

    <!-- Disable warning about existing shim in MaSch.Test -->
    <NoWarn>$(NoWarn);0436</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(GitSourceDir)Core/MaSch.Core.csproj" />
    <ProjectReference Include="$(GitSourceDir)Test/MaSch.Test.csproj" />
  </ItemGroup>

</Project>
